{
  "name": "sebastian/budget_manager",
  "description": "A PHP application for tracking expenses",
  "type": "project",
  "require": {
    "vlucas/phpdotenv": "^5.6",
    "gemini-api-php/client": "^1.7",
    "symfony/http-client": "^7.4",
    "nyholm/psr7": "^1.8",
    "psr/http-client": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Framework\\": "src/Framework",
      "App\\": "src/App/"
    },
    "files": [
      "src/App/Config/Routes.php",
      "src/App/Config/Middleware.php"
    ]
  },
  "scripts": {
    "budget_manager": "php cli.php"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
